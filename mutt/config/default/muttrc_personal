# IMAP settings
set imap_user = "username"
set imap_pass = "password"
set folder = "imaps://username@imap_host:993/"

#Mailbox settings
set spoolfile = "+INBOX"
set postponed = "+Drafts"
set record = "+INBOX"

#SMTP settings
set smtp_url = "smtp://smtp_hostname:25/"
set smtp_pass = "password"

# Personal settings
set from = "your@email.address"
set realname = "Real Name"
my_hdr Organization: Company, City, Country
